{
    "name": "methorz/markme",
    "description": "MethorZ - Markdown Parser",
    "type": "library",
    "homepage": "https://methorz.io",
    "license": "proprietary",
    "keywords": [
        "methorz",
        "markdown",
        "parser"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.19",
        "phpspec/prophecy-phpunit": "^2.2",
        "phpunit/phpunit": "^11.3",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MethorZ\\MarkMe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MethorZ\\MarkMeTest\\": "test/"
        }
    }
}
